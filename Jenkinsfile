//定义jenkins流水线
pipeline {
    agent any
    stages {
        stage('编译') {
            steps {
                echo "编译。。。"
            }
        }
        stage('构建') {
            steps {
                echo "构建。。。"
            }
        }
        stage('打包') {
            steps {
                echo "打包。。。"
            }
        }
        stage('部署') {
            steps {
                echo "部署。。。"
            }
        }
    }
}